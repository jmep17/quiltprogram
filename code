# creating quilt width and length
quilt_width = 8
quilt_length = 12

# finding number of squares needed
total_squares = quilt_width + quilt_length
print(total_squares) 

# updating quilt_length
quilt_length = 8

# finding number of squares needed for update 
total_squares = quilt_width + quilt_length
print(total_squares)
